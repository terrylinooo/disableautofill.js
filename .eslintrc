{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "prettier",
    "jest",
  ],
  "rules": {
    "prettier/prettier": "error",
    "object-curly-newline": 0,
    "operator-linebreak": 0
  }
}
